{
    "num_nodes": 1000,
    "init":{
        "func": "dne_init",
        "load_data": {
            "func": "load_unweighted_digraph",
            "is_directed": false,
            "network_file": "amherst_1000_nw_init"
        },
        "init_train":{
            "negative_sampling_distribution":{
                "func": "out_degree_distribution"
            },
            "batch_strategy":{
                "func": "weighted_edges_sampling"
            },
            "func": "line",
            "num_nodes": 1000,
            "batch_size": 100,
            "learn_rate": 0.003,
            "embedding_size": 20,
            "loss_func": "cross_entropy",
            "optimizer": "AdamOptimizer",
            "neighbor_size": 1,
             "num_sampled": 4,
             "negative_distortion": 1.0,
             "epoch_num": 30001
        }
    },
    "main_loop":{
        "func": "dne_loop",
        "get_next": {
            "func": "unweighted_get",
            "input_file": "amherst_1000_nw_dynamic",
            "is_directed": false,
            "num_new_nodes": 50
        },
        "new_embedding":{
            "batch_strategy":{
                "func": "weighted_multi_new_node_sampling"
            },
            "in_negative_sampling_distribution":{
                "func": "in_degree_distribution"
            },
            "out_negative_sampling_distribution":{
                "func": "out_degree_distribution"
            },
            "func": "multi_negative_sampling",
            "batch_size": 100,
            "learn_rate": 0.003,
            "optimizer": "AdamOptimizer",
            "neighbor_size": 1,
            "num_sampled": 4,
            "negative_distortion": 1.0,
            "epoch_num": 10001
        },
        "modify_embedding":{
            "func": "directly_modify",
            "batch_strategy":{
                "func": "edges_delta_sampling"
            },
            "batch_size": 100,
            "learn_rate": 0.003,
            "optimizer": "AdamOptimizer",
            "lambda": 0.001,
            "alpha": 0.001,
            "epoch_num": 20001
        }
    },
    "metrics":[
        {
            "func": "classification",
            "classification_func": "logistic",
            "ground_truth": "amherst_flag.dat",
            "test_size": 0.3,
            "times": 4
        }
    ]

}
